// Given an array of integers, return the sum of all elements.
  def array_sum(arr):
      total = 0
      for num in arr:
          total += num
      return total
        
print(array_sum([1, 2, 3, 4, 5]))
        
// 2. Find the Maximum Element in an Array/
def find_max(arr):
    max_val = arr[0]
    for num in arr:
        if num > max_val:
            max_val = num
    return max_val

print(find_max([10, 5, 20, 8]))
        
  // Reverse the elements of the array
    def reverse_array(arr):
    return arr[::-1]


print(reverse_array([1, 2, 3, 4, 5]))

// Count the number of even and odd numbers.
def count_even_odd(arr):
    even = odd = 0
    for num in arr:
        if num % 2 == 0:
            even += 1
        else:
            odd += 1
    return even, odd

even_count, odd_count = count_even_odd([1, 2, 3, 4, 5, 6])
print("Even:", even_count, "Odd:", odd_count) 

//Check if a Number Exists in an Array
 def exists_in_array(arr, target):
    for num in arr:
        if num == target:
            return True
    return False


print(exists_in_array([4, 6, 8, 2], 6))
 
//Find the Second Largest Element in an Array

def second_largest(arr):
    if len(arr) < 2:
        return None
    first = second = float('-inf')
    for num in arr:
        if num > first:
            second = first
            first = num
        elif first > num > second:
            second = num
    return second

print(second_largest([10, 20, 4, 45, 99]))

        
    
